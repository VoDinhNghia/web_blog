<div class="col-sm-4 text-center" id="study_left">
    <br>
    <ul class="nav nav-pills list-group flex-column">
        <li class="nav-item">
            <a class="nav-link active" href="#">Utilities</a>
        </li><br>
        <!-- <li class="list-group-item">
            <a class="bg-gray text-primary" href="/embed/search/article">
                Search article science
             </a>
        </li>
        <li class="list-group-item">
            <a class="bg-gray text-primary" href="/mybook">
                Read books online
             </a>
        </li> -->
        <li class="list-group-item">
            <a class="bg-gray text-primary" href="/myschedule">
                My schedule
             </a>
        </li>
    </ul>

    <hr class="d-sm-none"><br>
    <ul class="nav nav-pills list-group flex-column">
        <li class="nav-item active">
            <button type="button" class="btn btn-outline-primary" data-toggle="modal" data-target="#LogLogin" style="width: 100%;">Recycle</button>
            <div class="modal" id="LogLogin" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <form action="" method="POST">
                            <div class="modal-header">
                                <h4 class="modal-title text-primary">Recycle</h4>
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>
                            <div class="modal-body">
                                <table class="table table-hover">
                                    <thead>
                                        <tr class="text-primary">
                                            <th>No</th>
                                            <th>Name</th>
                                            <th>Restore</th>
                                            <th>Delete</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% if (recycle.length){
                                    for(let i = 0; i < recycle.length; i++){ 
                                    %>
                                            <tr>
                                                <td>
                                                    <%=i+1 %>
                                                </td>
                                                <td>
                                                    <%= recycle[i].NameTopic%>
                                                </td>
                                                <td>
                                                    <a href="/restore/topic/ <%= recycle[i].ID%>">
                                                        <button type="button" class="btn btn-outline-primary btn-sm">Restore</button>
                                                    </a>
                                                </td>
                                                <td>
                                                    <a href="/destroy/topic/<%= recycle[i].ID%>">
                                                        <button type="button" class="btn btn-outline-danger btn-sm">Delete</button>
                                                    </a>
                                                </td>
                                            </tr>
                                            <%}}else{%>
                                                <tr>
                                                    <td colspan="3" class="text-primary">Not found topic in recycle.</td>
                                                </tr>
                                                <%}%>
                                    </tbody>
                                </table>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-outline-danger" data-dismiss="modal">Close</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>

        </li>
    </ul>

    <hr class="d-sm-none"><br>
    <ul class="nav nav-pills list-group flex-column">
        <li class="nav-item active">
            <button type="button" class="btn btn-outline-primary float-right" data-toggle="modal" data-target="#RecycleRestore" style="width: 100%;">Log login</button>
            <div class="modal" id="RecycleRestore" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <form action="" method="POST">
                            <div class="modal-header">
                                <h4 class="modal-title text-primary">log login</h4>
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>
                            <div class="modal-body">
                                <table class="table table-hover">
                                    <thead>
                                        <tr class="text-primary">
                                            <th>last login</th>
                                            <th>curent login</th>
                                            <th>Device</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% if (logLogin.length){
                                    for(let i = 0; i < logLogin.length; i++){ 
                                    %>
                                            <tr>
                                                <td>
                                                    <%= (logLogin[i].LastLogin).slice(0,16)%>
                                                </td>
                                                <td>
                                                    <%= (logLogin[i].CurentLogin).slice(0,16)%>
                                                </td>
                                                <th>
                                                    <%= (logLogin[i].DeviceLogin).split(' ')[1]
                                                    +' '+(logLogin[i].DeviceLogin).split(' ')[2]
                                                    +' '+(logLogin[i].DeviceLogin).split(' ')[3]%>
                                                </th>
                                            </tr>
                                            <%}}else{%>
                                                <tr>
                                                    <td colspan="3" class="text-primary">Not found.</td>
                                                </tr>
                                                <%}%>
                                    </tbody>
                                </table>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-outline-danger" data-dismiss="modal">Close</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>

        </li>
    </ul>
</div>